{
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "vercel.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDit": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}